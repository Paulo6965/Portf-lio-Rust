fn main(){
    let matriz:Vec<Vec<i32>>=vec![
    vec![1,2,3],
    vec![4,5,6],
    vec![7,8,9],
    ];
    let valor = matriz[1][2]; //indice come√ßa em 0
    
    println!("Elemento na linha 1, coluna 2: {}", valor);
    println!("Elemento na linha 2,coluna 0: {}",matriz[2][0]);
    
    println!("Matriz {:?}",matriz);
}
